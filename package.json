  "scripts": {
    "clean": "node ./scripts/clean",
    "buildClient": "NODE_ENV=production BABEL_ENV=production node ./scripts/buildClient",
    "buildServer": "NODE_ENV=production BABEL_ENV=production node ./scripts/buildServer",
    "lint": "npm run lint-js && npm run lint-sass",
    "lint-js": "eslint --config .eslintrc ./src ./style-guide",
    "lint-sass": "sass-lint --config .sass-lint.yml -v",
    "start": "NODE_ENV=development BABEL_ENV=development node ./scripts/start",
    "prod": "NODE_ENV=production BABEL_ENV=production node ./scripts/production",
    "prepublish": "npm run clean && npm run prepublish-javascript && npm run prepublish-sass",
    "prepublish-javascript": "BABEL_ENV=production babel ./src --ignore example,*.test.js --out-dir ./lib && babel ./utils --out-dir ./lib/utils",
    "prepublish-sass": "NODE_ENV=production BABEL_ENV=production node ./scripts/prepublish-sass",
    "test": "NODE_ENV=production BABEL_ENV=production jest --config .jest.json"
  },
